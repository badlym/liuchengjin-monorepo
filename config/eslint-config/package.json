{
  "name": "@liuchengjin/eslint-config",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "import": "./dist/base.mjs",
      "require": "./dist/base.cjs"
    },

    "./vue-app": {
      "types": "./dist/vue-app.d.ts",
      "import": "./dist/vue-app.mjs",
      "require": "./dist/vue-app.cjs"
    },
    "./react-app": {
      "types": "./dist/react-app.d.ts",
      "import": "./dist/react-app.mjs",
      "require": "./dist/react-app.cjs"
    },

    "./react-native": {
      "types": "./dist/react-native.d.ts",
      "import": "./dist/react-native.mjs",
      "require": "./dist/react-native.cjs"
    },
    "./nest-app": {
      "types": "./dist/nest-app.d.ts",
      "import": "./dist/nest-app.mjs",
      "require": "./dist/nest-app.cjs"
    }

  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "pnpm rimraf .turbo node_modules dist",
    "init": "pnpm install",
    "lint": "pnpm eslint . --fix",
    "ncu": "ncu",
    "ncu:update": "ncu -u",
    "release": "release-it --ci",
    "stub": "pnpm unbuild --stub"
  },
  "dependencies": {
    "@liuchengjin/release-it-config": "workspace:^",
    "globals": "^15.0.0"

  },
  "devDependencies": {
    "typescript-eslint": "^7.6.0",
    "@eslint/js": "^9.0.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@unocss/eslint-config": "^0.59.1",
    "eslint": "^9.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.13.2",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.24.1",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "vue-eslint-parser": "^9.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
